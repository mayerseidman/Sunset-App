{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "scss": "node-sass --watch src/scss -o src/css"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "blob-stream": "^0.1.3",
    "bootstrap": "^4.0.0-alpha.2",
    "express": "^4.16.3",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "monk": "*",
    "node-fetch": "^2.6.0",
    "node-geocoder": "^3.23.0",
    "node-sass": "^4.12.0",
    "node-schedule": "^1.3.2",
    "node-sunsetwx": "0.0.1",
    "path": "^0.12.7",
    "pdf-image": "^2.0.0",
    "pdf-poppler": "^0.2.1",
    "pdf2pic": "^1.3.9",
    "pdfkit": "^0.10.0",
    "popper.js": "^1.15.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^5.0.1",
    "sunrise-sunset-js": "^2.1.0",
    "tether": "^1.4.7",
